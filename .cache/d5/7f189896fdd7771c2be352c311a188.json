tion","generated":{"line":79,"column":0},"source":"components/Suggestions.js","original":{"line":53,"column":8}},{"name":"section","generated":{"line":79,"column":6},"source":"components/Suggestions.js","original":{"line":53,"column":8}},{"generated":{"line":79,"column":13},"source":"components/Suggestions.js","original":{"line":53,"column":15}},{"name":"appendChild","generated":{"line":79,"column":14},"source":"components/Suggestions.js","original":{"line":53,"column":16}},{"generated":{"line":79,"column":25},"source":"components/Suggestions.js","original":{"line":53,"column":8}},{"name":"card","generated":{"line":79,"column":26},"source":"components/Suggestions.js","original":{"line":53,"column":28}},{"generated":{"line":79,"column":30},"source":"components/Suggestions.js","original":{"line":53,"column":8}},{"generated":{"line":80,"column":0},"source":"components/Suggestions.js","original":{"line":54,"column":5}},{"name":"Suggestions","generated":{"line":86,"column":15},"source":"components/Suggestions.js","original":{"line":58,"column":15}}],"sources":{"components/Suggestions.js":"import CityInput from \"./CityInput\";\n\nclass Suggestions {\n\n    createHtmlElement() {\n        let componentName = Suggestions.valueOf().name\n        const htmlSection = document.createElement('section')\n        htmlSection.id = componentName\n        const card = document.createElement('div');\n        const ville = document.createElement('span');\n        const departement = document.createElement('span');\n        card.setAttribute('class', 'flex flex-col min-w-1/6 h-16 px-2 items-center justify-center cursor-pointer');\n        departement.setAttribute('class', 'text-gray-400 text-sm')\n        card.appendChild(ville);\n        card.appendChild(departement);\n        ville.textContent = \"VILLE\"\n        departement.textContent=\"DEPARTEMENT\"\n        document.getElementById('main').appendChild(card)\n\n\n    }\n\n    searchForSuggestion(city) {\n        fetch(`https://geo.api.gouv.fr/communes?nom=${city}&fields=departement&boost=population&limit=5`)\n            .then(res => res.json())\n            .then(data => {\n                const section = document.querySelector('#suggestions');\n                section.innerHTML = '';\n                if (data) {\n                    data.forEach(o => this.generateSuggestion(o));\n                }\n            })\n            .catch(error => console.log(error));\n    }\n\n    generateSuggestion(data) {\n        const {\n            nom,\n            code,\n            departement: {\n                code: codeDepartement,\n                nom: nomDepartement\n            }\n        } = data;\n        departement.innerHTML = `${nomDepartement || '??'} (${codeDepartement || '??'})`;\n        card.addEventListener('click', () => {\n            $('#input-search').val(nom);\n            CityInput.searchCity(nom);\n            this.searchForSuggestion(nom);\n        });\n\n        const section = document.querySelector('#suggestions');\n        section.appendChild(card);\n    }\n\n}\n\nexport default Suggestions"},"lineCount":null}},"error":null,"hash":"733ba678e83bc01c7125140082f662ca","cacheData":{"env":{}}}